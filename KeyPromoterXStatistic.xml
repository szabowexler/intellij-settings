<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Find">
        <Item>
          <IdeaActionID>ActivateFindToolWindow</IdeaActionID>
          <ShortCut>'⌘3'</ShortCut>
          <Description>Find</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Rerun Failed Tests'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌃F5'</ShortCut>
          <Description>Rerun 'Rerun Failed Tests'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'SettingsManagerTest'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌃F5'</ShortCut>
          <Description>Rerun 'SettingsManagerTest'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'SettingsManagerTest'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⇧F10'</ShortCut>
          <Description>Run 'SettingsManagerTest'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'TeamSettingsIterableTest'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⇧F10'</ShortCut>
          <Description>Run 'TeamSettingsIterableTest'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>50</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>